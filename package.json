{
  "name": "treehouse-fsjs-project10-build-a-library-manager",
  "version": "0.1.0",
  "description": "### Working with databases — storing, retrieving, updating and deleting information — is an important software developer talent. Create a web application for searching, adding and updating items in a lending library, using JavaScript, Node.js, Express, and an Sqlite Database.",
  "main": "index.js",
  "dependencies": {
    "express": "^4.16.4",
    "express-validator": "^5.3.1",
    "helmet": "^3.15.0",
    "pug": "^2.0.3",
    "sequelize": "^4.42.0",
    "sequelize-cli": "^5.4.0",
    "sqlite3": "^4.0.6"
  },
  "devDependencies": {
    "nodemon": "^1.18.9",
    "prettier": "^1.16.1"
  },
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon --ignore node_modules --watch ./",
    "init:db": "./node_modules/.bin/sequelize db:migrate && ./node_modules/.bin/sequelize db:seed:all",
    "reinit:db": "./node_modules/.bin/sequelize db:migrate:undo:all && ./node_modules/.bin/sequelize db:migrate && ./node_modules/.bin/sequelize db:seed:all",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nvwebd/treehouse-fsjs-project10-build-a-library-manager.git"
  },
